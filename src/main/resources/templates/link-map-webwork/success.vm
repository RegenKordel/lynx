$webResourceManager.requireResource(
    "openreq.qt.issuelinkmap-jira-plugin.issuelinkmap-jira-plugin:issuelinkmap-jira-plugin-resources")
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html" charset=ISO-8859-1">
    <title>Lynx</title>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/vis/4.21.0/vis.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"
            integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T"
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/vis/4.21.0/vis.min.css">
</head>

<body>
<div class="topnav">
    <div class="link-container">
    ##        <a target="_blank" href="https://openreq.eu/" style="padding: 0"><img alt="or_logo"
    ##                                                                              src="../download/resources/openreq.qt.issuelinkmap-jira-plugin.issuelinkmap-jira-plugin:issuelinkmap-jira-plugin-resources/images/or_icon.png"
    ##                                                                              width="40px" height="40px"/></a>
    ##    <a target="_blank" href="https://bugreports.qt.io/browse/">Qt's Jira</a>

        <a class="button-effect" href="./HelpPageAction.jspa">Help</a>
        <a target="_blank" href="https://github.com/OpenReqEU/issuelinkmap-jira-plugin/issues">Report a Bug</a>
    </div>

    <div class="text-container">
        <small>Version 1.3.22.1</small>
    </div>

    <div class="search-container"></div>
</div>


<div class="container" style="padding-top: 30px">
    <div class="row">
        <h2>Lynx Prototype</h2>
    </div>

    <div class="col-10">
        <form action="./IssueSearchWebworkAction.jspa" method="get" id="search-id" style="display:inline-block;">
            <fieldset>
                <table style="border-collapse: collapse; text-align-all:left">

                    <tr>
                        <th>Issue Key</th>
                        <th></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><input type="text" name="issue" id="issue" required="required" width="200px"></td>
                        <td><input type="submit"
                                   class="button search button-effect"
                                   value="Search"></td>
                        <td>
                            <div class="col-2">
                                <div class="loader" id="loader"></div>
                            </div>
                        </td>
                    </tr>
                    <tr>

                    </tr>
                    <td><i>
                        <small>Enter an issue key.
                        </small>
                    </i></td>
                    <td><i></i></td>
                    <td></td>
                </table>
            </fieldset>
        </form>
    </div>
</div>
<div class="container" style="padding-top: 30px">
    <div class="row">
        <h2>Health Overview</h2>
    </div>
    <div class="row">
        <h3>Type Inconsistencies</h3>
    </div>
    <div class="row">
        <div class="col-4">
            <canvas id="typeInconsistencies" style="max-width: 500px;"></canvas>
        </div>
        <div class="col-8">
            <table style='width: 100%'>
                <tr>
                    <th>Issue</th>
                    <th>Problem</th>
                </tr>
                <tr>
                    <td>TCAP-274</td>
                    <td>Epic missing Sub-Tasks.</td>
                </tr>
                <tr>
                    <td>TCAP-272</td>
                    <td>Sub-Task without Epic.</td>
                </tr>
            </table>
        </div>
    </div>
    <div class="row">
        <h3>Resolution Inconsistencies</h3>
    </div>
    <div class="row">
        <div class="col-4">
            <canvas id="resInconsistencies" style="max-width: 500px;"></canvas>
        </div>
        <div class="col-8">
            <table style='width: 100%'>
                <tr>
                    <th>Issue</th>
                    <th>Problem</th>
                </tr>
                <tr>
                    <td>TCAP-274</td>
                    <td>Duplicate resolution without corresponding link.</td>
                </tr>
                <tr>
                    <td>TCAP-272 & TCAP-275</td>
                    <td>Linked by duplicate link but neither has resolution "duplicate".</td>
                </tr>
            </table>
        </div>
    </div>
    <div class="row">
        <h3>Comment Inconsistencies</h3>
    </div>
    <div class="row">
        <div class="col-4">
            <canvas id="commInconsistencies" style="max-width: 500px;"></canvas>
        </div>
        <div class="col-8">
            <table style='width: 100%'>
                <tr>
                    <th>Issue</th>
                    <th>Comment</th>
                    <th>Action</th>
                </tr>
                <tr>
                    <td>TCAP-274</td>
                    <td>"Isn't this related to issue TCAP-272?"</td>
                    <td>Add Link</td>
                </tr>
                <tr>
                    <td>TCAP-272</td>
                    <td>"I still face this issue, please reopen."</td>
                    <td>Investigate</td>
                </tr>
            </table>
        </div>
    </div>
    <div class="row">
        <h3>Link Inconsistencies</h3>
    </div>
    <div class="row">
        <div class="col-4">
            <canvas id="linkInconsistencies" style="max-width: 500px;"></canvas>
        </div>
        <div class="col-8">
            <table style='width: 100%'>
                <tr>
                    <th>Issue(s)</th>
                    <th>Problem</th>
                </tr>
                <tr>
                    <td>TCAP-233</td>
                    <td>No description.</td>
                </tr>
                <tr>
                    <td>TCAP-276 & TCAP-278</td>
                    <td>Multiple links with differing types between issues.</td>
                </tr>
            </table>
        </div>
    </div>
</div>

<script>
    $('#search-id').submit(function ()
    {
        $('#loader').show();
    });
    var typeLoc = document.getElementById("typeInconsistencies").getContext("2d");
    var typeChart = new Chart(typeLoc, {
        type: "bar",
        data: {
            labels: [
                "Epic Links",
                "Epic Issues",
                "Sub-Task Links",
                "Sub-Task Issues",
            ],
            datasets: [
                {
                    label: "Incorrect",
                    data: [0.0, 0.81, 0.70, 0.00],
                    backgroundColor: "#ce0000",
                    hoverBackgroundColor: "#e16666"
                },
                {
                    label: "Correct",
                    data: [1.0, 0.09, 0.30, 1.0],
                    backgroundColor: "#14882c",
                    hoverBackgroundColor: "#72B780"
                }
            ]
        },
        options: {
            responsive: true,
            scales:{
                xAxes: [{
                    stacked: true
                }],
                yAxes: [{
                    stacked: true
                }]
            }
        }
    });

    var resLoc = document.getElementById("resInconsistencies").getContext("2d");
    var resChart = new Chart(resLoc, {
        type: "bar",
        data: {
            labels: [
                "Duplicate Links",
                "Duplicate Issues"
            ],
            datasets: [
                {
                    label: "Incorrect",
                    data: [0.25, 0.2],
                    backgroundColor: "#ce0000",
                    hoverBackgroundColor: "#e16666"
                },
                {
                    label: "Correct",
                    data: [0.75, 0.8],
                    backgroundColor: "#14882c",
                    hoverBackgroundColor: "#72B780"
                }
            ]
        },
        options: {
            responsive: true,
            scales:{
                xAxes: [{
                    stacked: true
                }],
                yAxes: [{
                    stacked: true
                }]
            }
        }
    });

    var commLoc = document.getElementById("commInconsistencies").getContext("2d");
    var commChart = new Chart(commLoc, {
        type: "pie",
        data: {
            labels: [
                "Non-actionable issues",
                "Link actions",
                "Status actions",
            ],
            datasets: [
                {
                    data: [13000, 1500, 500],
                    backgroundColor: ["#14882c", "#ffd351", "#ce0000"],
                    hoverBackgroundColor: ["#72B780", "#ffe496", "#e16666"]
                }
            ]
        },
        options: {
            responsive: true,
            legend: {
                position: 'right',
                labels: {
                    padding: 15,
                    boxWidth: 10
                }
            }
        }
    });

    var linkLoc = document.getElementById("linkInconsistencies").getContext("2d");
    var linkChart = new Chart(linkLoc, {
        type: "pie",
        data: {
            labels: [
                "Correct Links",
                "No title",
                "No description",
                "Multiple links defined",
                "Conflicting link types",
            ],
            datasets: [
                {
                    data: [4000, 200, 100, 400, 200],
                    backgroundColor: ["#14882c", "#ce0000", "#ce0000", "#ffd351", "#ce0000"],
                    hoverBackgroundColor: ["#72B780", "#e16666", "#e16666", "#ffe496", "#e16666"]
                }
            ]
        },
        options: {
            responsive: true,
            legend: {
                position: 'right',
                labels: {
                    padding: 15,
                    boxWidth: 10
                }
            }
        }
    });

</script>
</body>
</html>